<p id="notice"><%= notice %></p>

<h1>Other Events Lists</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Point worth</th>
      <th>Event type</th>
      <th>Number participation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @other_events.each do |other_event| %>
      <tr>
        <td><%= other_event.event_id %></td>
        <td><%= other_event.point_worth %></td>
        <td><%= other_event.event_type %></td>
        <td><%= other_event.number_participation %></td>
        <td><%= link_to 'Show', other_event %></td>
        <td><%= link_to 'Edit', edit_other_event_path(other_event) %></td>
        <td><%= link_to 'Destroy', other_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other Events List', new_other_event_path %>
