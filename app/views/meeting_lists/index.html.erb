<p id="notice"><%= notice %></p>

<h1>Meeting Lists</h1>

<table>
  <thead>
    <tr>
      <th>Meeting</th>
      <th>Meeting name</th>
      <th>Date</th>
      <th>Location</th>
      <th>Start time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meeting_lists.each do |meeting_list| %>
      <tr>
        <td><%= meeting_list.meeting_id %></td>
        <td><%= meeting_list.meeting_name %></td>
        <td><%= meeting_list.date %></td>
        <td><%= meeting_list.location %></td>
        <td><%= meeting_list.start_time %></td>
        <td><%= link_to 'Show', meeting_list %></td>
        <td><%= link_to 'Edit', edit_meeting_list_path(meeting_list) %></td>
        <td><%= link_to 'Destroy', meeting_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting List', new_meeting_list_path %>
